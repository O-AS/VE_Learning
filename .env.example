# Environment Variables for Embeddings Visualizer

# Development
NODE_ENV=development
NEXT_PUBLIC_APP_NAME="Embeddings Visualizer"
NEXT_PUBLIC_APP_VERSION="1.0.0"

# API Configuration
NEXT_PUBLIC_API_BASE_URL="http://localhost:3000"
NEXT_PUBLIC_MAX_SENTENCES=50
NEXT_PUBLIC_MAX_REQUEST_SIZE=10000000

# TensorFlow.js Configuration
NEXT_PUBLIC_TENSORFLOW_BACKEND="webgl"
NEXT_PUBLIC_MODEL_CACHE_SIZE=1

# Visualization Settings
NEXT_PUBLIC_DEFAULT_VISUALIZATION_METHOD="pca"
NEXT_PUBLIC_DEFAULT_DIMENSIONS=2
NEXT_PUBLIC_DEFAULT_PERPLEXITY=30

# Analytics (Optional)
# NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=""
# NEXT_PUBLIC_CLARITY_PROJECT_ID=""

# Deployment
NEXT_PUBLIC_SITE_URL="https://your-app.vercel.app"
NEXT_PUBLIC_CANONICAL_URL="https://your-app.vercel.app"